{
    "name": "gam6itko/dpd-carrier",
    "description": "DPD carrier api library",
    "minimum-stability": "stable",
    "type": "library",
    "keywords": ["dpd", "carrier", "delivery", "russia"],
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Strizhak",
            "email": "gam6itko@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Gam6itko\\DpdCarrier\\": "src/Gam6itko/DpdCarrier"
        }
    },
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "ext-soap": "*"
    },
    "autoload-dev": {
        "psr-4": {
            "Gam6itko\\DpdCarrier\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7 | ^6.5"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    },
    "archive": {
        "exclude": ["/tests"]
    }
}